# Confluence Configuration
CONFLUENCE_URL=https://your-confluence-instance.atlassian.net
CONFLUENCE_USERNAME=your-email@example.com
CONFLUENCE_API_TOKEN=your-api-token-here
CONFLUENCE_DEFAULT_SPACE=YOURSPACE

# Provider Configuration
# Options: confluence-server, confluence-stub
CONFLUENCE_PROVIDER_TYPE=confluence-server

# Scheduler Configuration
# Interval in seconds for checking scheduled publications
SCHEDULER_INTERVAL_SECONDS=60
